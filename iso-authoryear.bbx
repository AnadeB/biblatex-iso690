\ProvidesFile{iso-authoryear.bbx}[2011/06/08]

\RequireBibliographyStyle{iso}
\ExecuteBibliographyOptions{%
  sorting=nyt
}

\def\printextrayear{%
  \printfield{year}\printfield{extrayear}%
}

\newbibmacro*{year-label}{%
  \ifnameundef{author}
    {\ifnameundef{editor}
       {}
       {\printextrayear}}
    {\printextrayear}%
}

% remove second appearance of year in a reference
\renewbibmacro*{date}{}%

\endinput
